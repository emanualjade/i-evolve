    "npm-run-all": "^4.0.2",
    "nsp": "^2.6.3",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router-dom": "^4.0.0-beta.8",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.4.2"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
